    <weiteres>
      <ROOT>
        <TCEforms><sheetTitle>Grundeinstellungen</sheetTitle></TCEforms>
        <type>array</type>
        	<el>
	          	<STORAGE_PID>
					<TCEforms>
						<label>Seiten-ID der Datensätzen</label>
						<config>
							<type>input</type>
							<eval>string</eval>
							<size>15</size>
